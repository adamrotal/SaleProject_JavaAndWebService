<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RESTLogin</servlet-name>
        <servlet-class>com.rest.RESTLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTToken</servlet-name>
        <servlet-class>com.rest.RESTToken</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTLogout</servlet-name>
        <servlet-class>com.rest.RESTLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTRegister</servlet-name>
        <servlet-class>com.rest.RESTRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTUser</servlet-name>
        <servlet-class>com.rest.RESTUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTPhoneNumber</servlet-name>
        <servlet-class>com.rest.RESTPhoneNumber</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTPostalCode</servlet-name>
        <servlet-class>com.rest.RESTPostalCode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTFullAddress</servlet-name>
        <servlet-class>com.rest.RESTFullAddress</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTFullName</servlet-name>
        <servlet-class>com.rest.RESTFullName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RESTUserById</servlet-name>
        <servlet-class>com.rest.RESTUserById</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RESTLogin</servlet-name>
        <url-pattern>/RESTLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTToken</servlet-name>
        <url-pattern>/RESTToken</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTLogout</servlet-name>
        <url-pattern>/RESTLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTRegister</servlet-name>
        <url-pattern>/RESTRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTUser</servlet-name>
        <url-pattern>/RESTUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTPhoneNumber</servlet-name>
        <url-pattern>/RESTPhoneNumber</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTPostalCode</servlet-name>
        <url-pattern>/RESTPostalCode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTFullAddress</servlet-name>
        <url-pattern>/RESTFullAddress</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTFullName</servlet-name>
        <url-pattern>/RESTFullName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RESTUserById</servlet-name>
        <url-pattern>/RESTUserById</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
